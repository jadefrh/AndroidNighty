<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jadefrh.nightylogin.GetSunsetActivity">


    <TextView
        android:id="@+id/patience"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Patience"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/sunset"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/sunset"
        app:layout_constraintBottom_toTopOf="@+id/sunset"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sunrise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sunrise of the day"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="302dp" />

    <TextView
        android:id="@+id/sunset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sunset of the day"
        tools:layout_editor_absoluteX="138dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="266dp" />

</android.support.constraint.ConstraintLayout>
