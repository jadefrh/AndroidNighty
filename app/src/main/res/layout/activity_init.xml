<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jadefrh.nightylogin.InitActivity">

    <TextView
        android:id="@+id/loggedin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="logged in"
        tools:layout_editor_absoluteY="103dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/fbaccesstoken"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="fbaccess"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/loggedin"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/loggedin"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/loggedin" />
</android.support.constraint.ConstraintLayout>
